public class EPattern {
    public static void main(String[] args) {
        int height = 7; // You can change the height of the letter "E"

        for (int i = 1; i <= height; i++) {
            for (int j = 1; j <= height; j++) {
                if (i == 1 || i == height || j == 1)
                    System.out.print("*");
                else
                    System.out.print(" ");
            }
            System.out.println();
        }
    }
}
