class Solution {
    public int removeElement(int[] arr, int val) {
     int j =0;

       for(int i=0; i<=arr.length-1; i++)
       {
            if(arr.length == 1 && arr[0] != val)
       { 
    
           arr[j] = arr[0];
           j++;
           break;
       }
           if(arr[i] != val)
           {
               arr[j] = arr[i];
               j++;
           }
       } 

      

       
       return j;

    }
}
