public class DivisibleBy3Substring {
    public static void main(String[] args) {
        String inputString = "abcdefghijk";

        for (int i = 0; i <= inputString.length() - 3; i += 3) {
            String substring = inputString.substring(i, i + 3);
            System.out.println("Divisible by 3 substring: " + substring);
        }
    }
}
